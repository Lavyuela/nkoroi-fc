{
  "expo": {
    "name": "Nkoroi FC Live Score",
    "slug": "nkoroi-fc-live-score",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/nkoroifclogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/nkoroifclogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#4FC3F7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nkoroifc.livescore"
    },
    "android": {
      "package": "com.nkoroifc.livescore",
      "versionCode": 5,
      "permissions": [
        "NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#4FC3F7"
      },
      "buildToolsVersion": "34.0.0",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "plugins": [
      "expo-notifications",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/firestore"
    ],
    "extra": {
      "eas": {
        "projectId": "7f3faf6f-5c89-4a7b-8bd5-03e48f0c6098"
      }
    }
  }
}
